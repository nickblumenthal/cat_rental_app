<form action="/cat_rental_requests" method = "post">
  <select name="cat_rental_request[cat_id]" id="cat_id">
    <% @all_cats.each do |cat| %>
      <option value="<%= cat.id %>"><%= cat.name %></option>
    <% end %>
  </select>
  <input type="date" name="cat_rental_request[start_date]" value = "Start Date">
  <input type="date" name="cat_rental_request[end_date]" value = "End Date">
  <input type="submit" value="Submit Request">
</form>
